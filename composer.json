{
    "name": "virlatinus/laravel-auditing-filesystem",
    "description": "This filesystem driver, designed for the owen-it/laravel-auditing package, facilitates storing audits in CSV files across all Laravel disks that are registered.",
    "keywords": [
        "audit",
        "auditing",
        "changes",
        "driver",
        "disk",
        "eloquent",
        "history",
        "log",
        "logging",
        "observer",
        "owen-it",
        "laravel",
        "record",
        "revision",
        "tracking",
        "filesystem",
        "csv",
        "storage"
    ],
    "type": "package",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "owen-it/laravel-auditing": "^14",
        "league/csv": "^9.28",
        "league/flysystem": "^3.31"
    },
    "config": {
        "platform": {
            "php": "8.2"
        }
    },
    "autoload": {
        "psr-4": {
            "VirLatinus\\Auditing\\Drivers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VirLatinus\\Auditing\\Drivers\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^10.9"
    }
}
